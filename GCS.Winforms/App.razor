<Microsoft.AspNetCore.Components.Routing.Router AppAssembly="@typeof(GCS.Winforms.Program).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData"  />
        @*<Microsoft.AspNetCore.Components.Routing.FocusOnNavigate RouteData="@routeData" Selector="h1"/>*@
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView>
            <p role="alert">Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Microsoft.AspNetCore.Components.Routing.Router>